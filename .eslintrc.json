{
    "env": {
        "node": true,
	"es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
	"no-console": "off",
	"no-redeclare": "off",
        "indent": [
            "error",
            "tab"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
